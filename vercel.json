{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "^en.*$"
        }
      ],
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "Accept-Language",
          "value": "^(?!^en).*"
        }
      ],
      "destination": "/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/en/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
