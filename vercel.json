{
    "version": 2,
    "builds": [
      {
        "src": "dist/influencer-frontend/browser/en",
        "use": "@vercel/static"
      },
      {
        "src": "dist/influencer-frontend/server/en",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/influencer-frontend/browser/en/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "dist/influencer-frontend/server/en/$1"
      }
    ]
  }
  